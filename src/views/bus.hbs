<div class="container">
  <h1>Available Buses</h1>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">From</th>
        <th scope="col">To</th>
        <th scope="col">Action</th>
      </tr>
    </thead>
    <tbody>
      {{#each data}}
      <tr>
        <th scope="row">{{this.id}}</th>
        <td> {{this.Name}}</td>
        <td>{{this.From}}</td>
        <td>{{this.To}}</td>
        <td><a href="/view-seats/{{this.id}}">view seats</a></td>
      </tr>
      {{/each}}
    </tbody>
  </table>

</div>